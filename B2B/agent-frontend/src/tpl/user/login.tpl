<div class="agent-login">   
        <form action="" style="width:440px" class="form">
            <p class="col66 bold Raleway f18 mb20 none" data-i18n="user_log_with_email">Log in with Email</p>

            <div class="mb30 mt45">
                <input type="text" class="br5 h36 lh36 shadow-input w410" placeholder="agent-agentlogin-phonenumber-placeholder" name="email" data-property-name="emailOrPhone"  data-control-type="TextBox">
            </div>
            <div class="mb30">
                <input type="password" class="br5 h36 lh36 shadow-input w410" placeholder="user_password" name="password" data-property-name="password"  data-control-type="TextBox">
            </div>
            <div class="js-img-code displayNone">
                <img src="" id="code" class="intValidateImg ml10 curd js-changeCode">
                <img src="images/captcha.png" class="va_10 js-changeCode cup">
                <input type="text" class="br5 w190 f18 h34 lh36 pl10 shadow-input" style="width: 232px; margin-left: 12px;" placeholder="user_enter_captcha" name="checkCode">
            </div>

            <p class="mt40 mb30">
                <input type="button" class="default_button f18 w164" data-i18n="user_log_in" id="login-submit" text="JOIN NOW">
                <input type="hidden" name="rememberMe" data-property-name="rememberMe"  data-control-type="TextBox" value="0">           
            </p>
            <a href="" style="color:#4582c4" class="f18 none" data-i18n="user_lost_password">Lost Your Password?</a>
        </form>
    <a href="javascript:void(0);" class="col66 f35 poa r0 t0 mr20 js-signIn">x</a>
</div>